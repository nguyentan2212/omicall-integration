{
	"name": "omicall-integration",
	"description": "Integration with omicall",
	"icon": "extension",
	"version": "1.0.1",
	"license": "MIT",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-bundle"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "interface",
				"name": "omicall-button-interface",
				"source": "src/omicall-button-interface/index.ts"
			},
			{
				"type": "hook",
				"name": "omicall-init",
				"source": "src/omicall-init/index.ts"
			}
		],
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate",
		"add": "directus-extension add"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "16.0.0",
		"@types/node": "^24.2.1",
		"typescript": "^5.9.2",
		"vue": "^3.5.18"
	}
}
